#include <iostream>
using namespace std;

int main() {
    int times[] = {80, 135, 150, 212};
    for (int i = 0; i < 4; i++) {
        int pos = (times[i] - 45) % 103;
        if (pos < 0) pos += 103; 

        int lampu;
        if (pos <= 2) lampu = 1;       // Kuning
        else if (pos <= 82) lampu = 2; // Merah
        else lampu = 3;                // Hijau

        cout << times[i] << " detik -> " << lampu << endl;
    }
    return 0;
}
